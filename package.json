    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "0.14.3",
    "jest": "22.1.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-hot-loader": "3",
    "redbox-react": "^1.6.0",
