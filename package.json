{
  "name": "appengine-listings-admin",
  "version": "0.1.0",
  "description": "appengine-listings-admin",
  "main": "frontend/dist/app.js",
  "scripts": {
    "start": "concurrently --kill-others \"pip install -U -t vendor/ -r requirements.txt && dev_appserver.py app.yaml\" \"webpack-dev-server --content-base dist --config frontend/configs/webpack.dev.js\"",
    "clean": "rimraf dist",
    "build:client": "webpack -p --config frontend/configs/webpack.prod.js",
    "build": "webpack --config frontend/configs/webpack.dev.js",
    "local": "concurrently \"npm run dev:client\" \"npm run dev --prefix frontend/mock-server\" ",
    "dev": "concurrently \"npm run dev:server --prefix\" \"npm run dev:client\"",
    "dev:client": "webpack-dev-server --content-base dist --config frontend/configs/webpack.dev.js",
    "test": "jest --config frontend/jest.json",
    "test:watch": "jest --config frontend/jest.json --watch"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.0.1",
    "bootstrap": "^4.1.1",
    "dateformat": "^3.0.3",
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-spinners": "^0.3.2",
    "reactjs-popup": "^1.1.1",
    "reactstrap": "^6.0.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "0.17.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "tslib": "1.8.1",
    "uglifyjs-webpack-plugin": "^1.2.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
